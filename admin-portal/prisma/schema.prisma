generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Document {
  id        Int      @id @default(autoincrement())
  fileName     String
  fileType  String
  fileSize  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}