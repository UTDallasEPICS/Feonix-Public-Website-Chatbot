generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model File {
  id     Int      @id @default(autoincrement())
  originalName     String
  storedName  String
  fileSize  Int
  fileType  String?
  savedTo String
  uploadedAt DateTime @default(now())
}